{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./resources/js",
    "paths": {
      "@/*": [
        "*"
      ]
    },
    "allowJs": true,
    "outDir": "./dist"
  }
}